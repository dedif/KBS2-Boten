<UserControl x:Class="BootRegistratieSysteem.Views.DashboardView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BootRegistratieSysteem.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="DashboardLayout" Background="White">
        <TabControl TabStripPlacement="top">
            <TabItem Header="Afschrijven">
                <Label Content="Content goes here..." />
            </TabItem>
            <TabItem Header="Leden" />
            <TabItem Header="Iets" />
        </TabControl>
        <Label Content="Hallo " HorizontalAlignment="Left" Margin="592,54,0,0" VerticalAlignment="Top"/>
        
    </Grid>
</UserControl>
